<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.pexesofx.EndGameController">
   <children>
      <Button fx:id="ReplayButton" layoutX="99.0" layoutY="267.0" mnemonicParsing="false" onMouseClicked="#handleReplayButton" prefHeight="52.0" prefWidth="116.0" text="REPLAY" textFill="#0a0a0a">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
      </Button>
      <Button fx:id="EndBotton" layoutX="364.0" layoutY="267.0" mnemonicParsing="false" onMouseClicked="#handleEndBotton" prefHeight="52.0" prefWidth="123.0" text="END" />
      <Label fx:id="GameOverLabel" alignment="CENTER" layoutX="166.0" layoutY="32.0" prefHeight="80.0" prefWidth="266.0" text="Game over" textFill="#dc510b">
         <font>
            <Font name="Elephant" size="48.0" />
         </font>
      </Label>
      <Label fx:id="EvaluationLable" alignment="CENTER" layoutX="204.0" layoutY="125.0" prefHeight="47.0" prefWidth="169.0" text="WIN">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </Label>
      <Label fx:id="yourScore" layoutX="99.0" layoutY="199.0" prefHeight="27.0" prefWidth="154.0" text="Your score:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="OpponentScore" layoutX="364.0" layoutY="199.0" prefHeight="27.0" prefWidth="177.0" text="Opponent score:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="smile" fitHeight="72.0" fitWidth="97.0" layoutX="253.0" layoutY="183.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@drawSmile.jpg" />
         </image></ImageView>
   </children>
</AnchorPane>
